<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="TechSolus – IT Solutions for Cloud, Cybersecurity, and Software Development.">
    <meta name="keywords" content="TechSolus, Coming Soon, IT Services, Cloud, Cybersecurity, Development">

    <title>TechSolus – IT Services, Cybersecurity, Cloud Solutions, and Software Development</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&display=swap" rel="stylesheet">

    <link rel="shortcut icon" type="image/ico" href="img/favicon.png" />
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/animate.css">
    <link rel="stylesheet" href="css/magnific-popup.css">
    <link rel="stylesheet" href="js/zoom-slider/vegas.min.css">
    <link rel="stylesheet" href="css/typed.css">
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="css/font-awesome.min.css" rel="stylesheet">
    <link href="style.css" rel="stylesheet">
    <link href="css/responsive.css" rel="stylesheet">
    <script src="js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body class="transparent-layer zoom-slider-bg">

    <div class="preeloader">
        <div class="preloader-spinner">
            <img src="img/loading.svg" alt="Loading...">
        </div>
    </div>

    <div class="main-area" id="home">
        <div class="main-area-bg"></div>

        <div class="welcome-text-area">
            <div class="container">
                <div class="row">
                    <div class="col-md-10 col-lg-10 col-md-offset-1 col-lg-offset-1 col-sm-12 col-xs-12">
                        <div class="welcome-text text-center">
                            <div class="clock-countdown">
                                <div class="site-config" data-date="07/01/2025 00:00:00" data-date-timezone="+0"></div>
                                <div class="days-counter">
                                    <div class="digit">
                                        <span class="days">00</span>
                                        <span class="txt">days</span>
                                    </div>
                                </div>
                                <div class="hour-counter">
                                    <div class="border"></div>
                                    <span class="hours">00</span><span class="normal">H</span>
                                    <span class="minutes">00</span><span class="normal">MN</span>
                                    <span class="seconds">36</span><span class="normal">S</span>
                                </div>
                            </div>
                            <h3>We Are</h3>
                            <h1 class="visible-xs">Coming Soon</h1>
                            <h1 class="hidden-xs cd-headline clip is-full-width">
                                <span class="cd-words-wrapper">
                                    <b class="is-visible">Empowering IT Solutions</b>
                                    <b>Cloud & Cybersecurity Experts</b>
                                    <b>Let’s Build the Future</b>
                                </span>
                            </h1>

                            <!-- CTA Button -->
                            <a href="mailto:info@techsolus.io" class="btn-primary">Contact Us</a>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="social-book-mark">
            <ul>
                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                <li><a href="#"><i class="fa fa-pinterest"></i></a></li>
            </ul>
        </div>
    </div>

    <!-- Scripts -->
    <script src="js/vendor/jquery-1.12.4.min.js"></script>
    <script src="js/vendor/bootstrap.min.js"></script>
    <script src="js/vendor/jquery.easing.1.3.js"></script>
    <script src="js/vendor/jquery-migrate-1.2.1.min.js"></script>
    <script src="js/typed.js"></script>
    <script src="js/jquery.downCount.js"></script>
    <script src="js/jquery.nicescroll.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/zoom-slider/vegas.min.js"></script>
    <script src="js/main.js"></script>

    <script>
        $(document).on('ready', function () {
            $('.zoom-slider-bg').vegas({
                slides: [
                    { src: 'img/background/default_5.jpg' },
                    { src: 'img/background/default_3.jpg' },
                    { src: 'img/background/default_6.jpg' }
                ],
                transition: 'fade',
                animation: 'random',
                transitionDuration: 4000,
                delay: 8000,
                timer: false
            });
        });
    </script>
</body>
</html>

